    <enum name="curve_type" item-prefix="sim_customui_curve_type_" base="20400">
        <item name="time" />
        <item name="xy" />
    </enum>
    <enum name="curve_style" item-prefix="sim_customui_curve_style_" base="22400">
        <item name="scatter" />
        <item name="line" />
        <item name="line_and_scatter" />
        <!-- following styles are only available in time plots: -->
        <item name="step_left" />
        <item name="step_center" />
        <item name="step_right" />
        <item name="impulse" />
    </enum>
    <enum name="curve_scatter_shape" item-prefix="sim_customui_curve_scatter_shape_" base="28400">
        <item name="none" />
        <item name="dot" />
        <item name="cross" />
        <item name="plus" />
        <item name="circle" />
        <item name="disc" />
        <item name="square" />
        <item name="diamond" />
        <item name="star" />
        <item name="triangle" />
        <item name="triangle_inverted" />
        <item name="cross_square" />
        <item name="plus_square" />
        <item name="cross_circle" />
        <item name="plus_circle" />
        <item name="peace" />
    </enum>
    <struct name="curve_options">
        <description>Extra options for the curve</description>
        <param name="scatter_shape" type="int" default="sim_customui_curve_scatter_shape_disc">
            <description>scatter shape</description>
        </param>
        <param name="scatter_size" type="int" default="10">
